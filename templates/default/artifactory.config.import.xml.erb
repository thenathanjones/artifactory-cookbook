<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<config xmlns="http://artifactory.jfrog.org/xsd/1.5.11" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.jfrog.org/xsd/artifactory-v1_5_11.xsd">
    <% if node[:artifactory][:external_url][:host] %>
        <serverName><%= node[:artifactory][:external_url][:host] %></serverName>
    <% end %>
    <offlineMode>false</offlineMode>
    <fileUploadMaxSizeMb>100</fileUploadMaxSizeMb>
    <dateFormat>dd-MM-yy HH:mm:ss z</dateFormat>
    <addons>
        <showAddonsInfo>true</showAddonsInfo>
        <showAddonsInfoCookie>1431916437304</showAddonsInfoCookie>
    </addons>
    <% if node[:artifactory][:mail][:host] %>
        <mailServer>
            <enabled>true</enabled>
            <host><%= node[:artifactory][:mail][:host] %></host>
            <port><%= node[:artifactory][:mail][:port] %></port>
            <subjectPrefix>[Artifactory]</subjectPrefix>
            <tls>false</tls>
            <ssl>false</ssl>
            <artifactoryUrl><%= node[:artifactory][:external_url][:protocol] %>://<%= node[:artifactory][:external_url][:host] %>:<%= node[:artifactory][:external_url][:port] %>/artifactory</artifactoryUrl>
        </mailServer>
    <% end %>
    <security>
        <anonAccessEnabled>true</anonAccessEnabled>
        <anonAccessToBuildInfosDisabled>false</anonAccessToBuildInfosDisabled>
        <hideUnauthorizedResources>false</hideUnauthorizedResources>
        <passwordSettings>
            <encryptionPolicy>supported</encryptionPolicy>
        </passwordSettings>
        <% if node[:artifactory][:ldap][:host] %>
            <ldapSettings>
                <ldapSetting>
                    <key>LDAP</key>
                    <enabled>true</enabled>
                    <ldapUrl><%= node[:artifactory][:ldap][:protocol] %>://<%= node[:artifactory][:ldap][:host] %>:<%= node[:artifactory][:ldap][:port] %></ldapUrl>
                    <search>
                        <searchFilter>(<%= node[:artifactory][:ldap][:search_filter] %>={0})</searchFilter>
                        <searchBase><%= node[:artifactory][:ldap][:base] %></searchBase>
                        <searchSubTree>true</searchSubTree>
                        <managerDn><%= node[:artifactory][:ldap][:bind_dn] %></managerDn>
                        <managerPassword><%= node[:artifactory][:ldap][:bind_pass] %></managerPassword>
                    </search>
                    <autoCreateUser>true</autoCreateUser>
                    <emailAttribute>mail</emailAttribute>
                </ldapSetting>
            </ldapSettings>
        <% end %>
        <ldapGroupSettings/>
    </security>
    <backups>
        <backup>
            <key>backup-daily</key>
            <enabled>true</enabled>
            <cronExp>0 0 2 ? * MON-FRI</cronExp>
            <retentionPeriodHours>0</retentionPeriodHours>
            <createArchive>false</createArchive>
            <excludedRepositories>
                <repositoryRef>jcenter</repositoryRef>
            </excludedRepositories>
            <sendMailOnError>true</sendMailOnError>
            <excludeBuilds>false</excludeBuilds>
            <excludeNewRepositories>false</excludeNewRepositories>
        </backup>
        <backup>
            <key>backup-weekly</key>
            <enabled>false</enabled>
            <cronExp>0 0 2 ? * SAT</cronExp>
            <retentionPeriodHours>336</retentionPeriodHours>
            <createArchive>false</createArchive>
            <excludedRepositories>
